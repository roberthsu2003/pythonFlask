   "source": [
    "#建立一個次執行緒執行的function\n",
    "import time\n",
    "def countdown(n):\n",
    "    while n > 0:\n",
    "        print(f\"倒數計時:{n}\")\n",
    "        n -= 1\n",
    "        time.sleep(1)\n",
    "\n",
    "from threading import Thread\n",
    "t_instance = Thread(target=countdown,args=(10,))\n",
    "t_instance.start()\n",
    "\n"
   ]
  }
